<div class="panel panel-default">
  <div class="panel-heading">
<h1>メッセージ一覧</h1>
  </div>
    
  <div class="panel-body">
<% @messages.each do |message| %>


<span class="label label-info"><%= message.name %></span>

<%= message.body %>
  <br>
<% end %>


<%= form_for(@message) do |f|  %>
         名前:
         <%= f.text_area :name %>
         内容:
         <%= f.text_area :body %>
<%= f.submit  %>
<% end %>
